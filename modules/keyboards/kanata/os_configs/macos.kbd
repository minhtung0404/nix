(defalias
  cmd lmet
  open_web (cmd open -a LibreWolf.app)
  open_discord (cmd open -a Discord.app)
  open_telegram (cmd open -a Telegram.app)
  open_vscode (cmd open -a "Visual Studio Code.app")
